<%- include('./partials/header')%>
<%- include('nav.ejs')%>
<section>
<form method="post">

<table class="table table-borderes w-50 mx-auto table-primary text-center mt-3">
<thead><tr>
    <td colspan="2">Add User Form</td>
</tr></thead>
<tbody>
    <tr>
        <td>User Name</td>
        <td ><input type="text" name="unm" value="<%= userData.userName %>"></td>
    </tr>
    
     <tr>
        <td>Email</td>
        <td ><input type="email" name="mailId" value="<%= userData.emailId %>"></td>
    </tr>
     <tr>
        <td>Password</td>
        <td ><input type="text" name="pwd" value="<%= userData.password %>"></td>
    </tr>
    <tr>
        <td ><input type="submit" name="addBtn" btn-primary  value="update"></td>
    </tr>
</tbody>
</table>
</form>
<%if(msg){%>
<p class="alert alert-success w-50 mx-auto">
    <%= msg%>
</p>
<%}%>
</section>
<%- include('./partials/footer')%>